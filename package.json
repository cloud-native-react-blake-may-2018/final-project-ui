{
  "name": "quiz",
  "version": "1.0.0",
  "description": "create and take quizzes",
  "main": "index.ts",
  "scripts": {
    "start": "npm run serve & npm run test",
    "serve": "ts-node index.ts",
    "test": "jest --watchAll",
    "build": "webpack"
  },
  "author": "Eric",
  "license": "ISC",
  "jest": {
    "roots": [
      "<rootDir>/client"
    ],
    "files": [
      "client/app/typings/custom.d.ts"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/client/app/tests/setupTests.ts"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/client/app/tests/setupTests.ts",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome": "latest",

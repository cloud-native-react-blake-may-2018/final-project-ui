    "react-image-gallery": "latest",
    "react-input-autosize": "latest",
    "react-redux": "latest",
    "react-router-dom": "latest",
    "react-router-hash-link": "latest",
    "react-scroll": "latest",
    "react-sidebar": "latest",
    "react-spinkit": "latest",
    "react-toggle": "latest",
    "reactstrap": "latest",
    "redux": "latest",
    "redux-thunk": "latest",
    "webpack": "latest",
    "webpack-hot-middleware": "latest"
  },
